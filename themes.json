      },
      "animations": {
        "transition": "background 0.3s ease",
        "hoverTransition": "background-color 0.3s ease, box-shadow 0.3s ease, transform 0.3s ease"
      },
      "borderAndShadow": {
        "focusBorderColor": "#007bff",
        "focusBoxShadow": "0 0 8px rgba(0, 123, 255, 0.5)"
      },
      "miscellaneous": {
        "opacity": "1"
      }
    }
